                            <span class="hidden-xs">Bienvenido, @User.Identity.Name</span>
                        </a>
                        <ul class="dropdown-menu animated fadeInLeft">
                            <li class="arrow"></li>

                            <li class="divider"></li>
                            <li><a href="@Url.Action("LogOff", "Account")">Cerrar Sesion</a></li>
                            @*<li><a href="javascript:;">Log Out</a></li>*@
                        </ul>
                    </li>
                </ul>
                <!-- end header navigation right -->
            </div>
            <!-- end container-fluid -->
        </div>
