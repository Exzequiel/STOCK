    <!-- begin #page-loader -->
    <div id="page-loader" class="fade in"><span class="spinner"></span></div>
    <!-- end #page-loader -->
    <!-- begin #page-container -->

    <div id="MyModal" class="modal fade" role="dialog" data-backdrop='static' data-keyboard="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"> <big>&times;</big></button>
                    <h4 class="modal-title" id="MyModalTitle">Modal Header</h4>
                </div>
                <div class="modal-body" id="MyModalBody">
                    <p>Some text in the modal.</p>
                </div>
            </div>
        </div>
    </div>

    <div class="modal" id="modalWaitingTime" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="">
        <div class="modal-dialog" role="document" style="margin-top: 15%;">
            <div class="modal-body text-center">
                <div class="material-loader">
                    <svg class="circular" viewBox="25 25 50 50" style="color: #fff;">
                        <circle style="color: #fff;" class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
                    </svg>
                    <div class="message" style="color: #fff;">Cargando...</div>
                </div>
            </div>
        </div>
    </div>


    <!-- begin #page-loader -->
    @*<div id="page-loader" class="fade in"><span class="spinner"></span></div>*@
    <!-- end #page-loader -->


    <div id="page-container" class="fade page-sidebar-fixed page-header-fixed">
